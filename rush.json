{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.68.0",
  "yarnVersion": "1.22.17",

  "pnpmOptions": {
    "strictPeerDependencies": true,
    "useWorkspaces": false
  },

  "nodeSupportedVersionRange": ">=14.12.0 <15.0.0",
  "ensureConsistentVersions": true,
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 2,

  "gitPolicy": {},

  "repository": {
    "url": "https://github.com/MDAkramSiddiqui/itnoa-webV1",
    "defaultBranch": "master",
    "defaultRemote": "origin"
  },

  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },

  "variants": [],
  "projects": [
    {
      "packageName": "@itnoa/common-styles-v1",
      "projectFolder": "styles/common-styles-v1",
      "shouldPublish": false
    }
  ]
}
