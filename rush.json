{
    "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
    "rushVersion": "5.68.0",
    "pnpmVersion": "6.32.11",

    "pnpmOptions": {
        "strictPeerDependencies": true,
        "useWorkspaces": false
    },

    "nodeSupportedVersionRange": ">=14.12.0 <15.0.0",
    "ensureConsistentVersions": true,
    "projectFolderMinDepth": 2,
    "projectFolderMaxDepth": 2,

    "gitPolicy": {
        "changeLogUpdateCommitMessage": "chore: updated change log [skip ci]",
        "versionBumpCommitMessage": "chore: Bump version [skip ci]"
    },

    "repository": {
        "url": "https://github.com/MDAkramSiddiqui/itnoa-webV1",
        "defaultBranch": "master",
        "defaultRemote": "origin"
    },

    "eventHooks": {
        "preRushInstall": [],
        "postRushInstall": [],
        "preRushBuild": [],
        "postRushBuild": []
    },

    "variants": [],
    "projects": [
        {
            "packageName": "@itnoa/common-styles-v1",
            "projectFolder": "styles/common-styles-v1",
            "shouldPublish": false,
            "versionPolicyName": "indi-basic"
        },
        {
            "packageName": "@itnoa/dev.mdakram-v1",
            "projectFolder": "apps/portfolio-v1",
            "shouldPublish": false,
            "versionPolicyName": "indi-basic"
        }
    ]
}
